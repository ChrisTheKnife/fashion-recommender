    "QUERY =\"\"\"SELECT DATE(t_dat) AS Date, COUNT(customer_id) AS Customer_count\n",
    "                FROM `data-buckeye-347917.fashion_recommender.transaction`\n",
    "                GROUP BY t_dat\n",
